class UserEvent < ActiveRecord::Base
  belongs_to :user

  validates_uniqueness_of :user_id,:scope =>:event_id
  validate  :validate

   def validate
     unless self.start_date.nil? or self.end_date.nil?
       errors.add(:end_date, "can not be before the start time") if self.end_date < self.start_date
     end
   end
end
