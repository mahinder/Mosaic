<div class="block-border" id="outer_blocks">
	<div class="block-content form">
		<h1>School Calendar</h1>
		<div class="block-controls enoch_block">
			<ul class="controls-tabs js-tabs">
				<li >
					<a href="<%= dashboard_path(@user) %>" title ="Back"> <img src="/assets/icons/web-app/32/Back.png" width="24" height="24"></a>
				</li>
			</ul>
		</div>
		<% unless flash[:warnnotice].nil? %>
		<ul class="message warning">
			<li>
				<strong><%= flash[:warnnotice] %></strong>
			</li>
			<li class="close-bt"></li>
		</ul>
		<% end %>
		<%= form_for(@calendar) do |f| %>
		<% if @calendar.errors.any? %>
		<div id="error_explanation">
			<h2><%= pluralize(@calendar.errors.count, "error") %> prohibited this calendar from being saved:</h2>
			<ul>
				<% @calendar.errors.full_messages.each do |msg| %>
				<li>
					<%= msg %>
				</li>
				<% end %>
			</ul>
		</div>
		<% end %>
		<div id="calendar" >
			<%= render :partial => 'month'%>
		</div>
		<div id="modal-box" style="display:none;"></div>
		<% end %>
	</div>
</div>
