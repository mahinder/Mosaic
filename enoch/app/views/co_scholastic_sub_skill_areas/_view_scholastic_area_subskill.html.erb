<div id="view_details">

	<div class="block-content form">
		<%unless @co_scholastic_sub_skill_areas.nil?%>
	<table class="table view_grading_details no-margin" id="" cellspacing="0" width="100%">
			<thead>
				<tr>
					<th class="black-cell"></th>
					<th scope="col">Co Scholastic Area sub Skill Name</th>
					<th scope="col">Status</th>
					<th scope="col">Action</th>
				</tr>
			</thead>
			<tbody>
				<%@co_scholastic_sub_skill_areas.each do |area_skill|%>
				<tr>
					<td class="th table-check-cell"></td>
					<td id="subSkill_name_<%= area_skill.id %>"><%= area_skill.co_scholastic_sub_skill_name%></td>
					<td><%= area_skill.is_active?  ? "True" : "False"%></td>
					<td><input type="hidden" name="object_id" value="<%= area_skill.id %>"/>
						<a href="#" id="update-href-<%= area_skill.id %>" status="<%= area_skill.is_active%>" title="Edit" class="with-tip update-area_sub_skill-href"><img src="/assets/icons/fugue/pencil.png" width="16" height="16" title="Edit"></a><a href="#" id="delete-href-<%= area_skill.id %>" title="Delete" class="with-tip delete-area_sub_skill"><img src="/assets/icons/fugue/cross-circle.png" width="16" height="16" title="Delete"></a>
						<a href="#" id="add-indicator-href-<%= area_skill.id %>" title="ADD Indicator" class="with-tip add_indicator_value-href"><img src="/assets/icons/add-mini.png" width="16" height="16" title="Add Indicator"></a>
					</td>
				</tr>
				<%end%>
			</tbody>
		</table>
		<%end%>
	</div>
<input type="hidden" id="url_prefix" name="url_prefix" value="/co_scholastic_sub_skill_areas" />
</div>
