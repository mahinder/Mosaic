<fieldset class="grey-bg no-margin">
	<legend>
		Action on create
	</legend>
	<div class="columns">
		<div class="colx2-left">
			<input type="hidden" id = "course_id_text" name = "object_id"  value="<%= course.id%>"/>
			<button id="update-course-href-<%= course.id%>" title="Edit" class="with-tip update-course-href" data-course_for = "<%= course_for%>" data-course_id = "<%= course.id%>" data-name = "<%= course.course_name %>" data-code = "<%= course.code%>" data-section = "<%= course.section_name%>" type="button" >
				Edit
			</button>
			<button id="delete-course-tab-href-<%= course.id%>" title="Delete" class="with-tip delete-course-tab-href" data-course-for = "<%= course_for%>" type="button">
				Delete
			</button>
		</div>
	</div>
</fieldset>
