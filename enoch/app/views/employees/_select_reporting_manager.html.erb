
	
	<% unless @employee.blank? %>
<ul class="mini-blocks-list">
	<% @employee.each_with_index do |emp1, i| %>
	<li>
		<input type="checkbox" id="a_<%= emp1.id %>" class = "select_employee" data-id ="<%= emp1.id %>" data-name="<%= emp1.first_name + ' ' + emp1.last_name %>"/>
		&nbsp; <%= emp1.full_name%> <ul class="tags float-right"></ul>
	</li>
	<%end%>
</ul>
<%else%> <h2>Not found</h2>
<%end%> 

