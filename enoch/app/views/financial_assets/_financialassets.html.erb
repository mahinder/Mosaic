				<form class="block-content form" id="table_form" method="post" action="">
					<h1>Financial Assets List</h1>
					<div>
						<ul class="tabs js-tabs same-height">
							<li class="current"><a href="#tab-active" title="Active">Active</a></li>
							<li><a href="#tab-inactive" title="Inactive">Inactive</a></li>
						</ul>
					</div>
					<div id="tabs-content" class="tabs-content">
						<div id="tab-active">
							<table id="assets-active-table" class="table financetable no-margin" cellspacing="0" width="100%">
								<thead>
									<%= render 'table_head' %>
								</thead>
									
									<% @active_financial_assets.each do |financial_asset| %>
										<%= render 'table_row', :financial_asset => financial_asset %>
									<% end %>
								
							</table>
						</div>
						<div id="tab-inactive">
							<table id="assets-inactive-table" class="table financetable no-margin" cellspacing="0" width="100%">
								<thead>
									<%= render 'table_head' %>
								</thead>
										
									<% @inactive_financial_assets.each do |financial_asset| %>
										<%= render 'table_row', :financial_asset => financial_asset %>
									<% end %>
								
							</table>
						</div>
					</div>
					<input type="hidden" id="url_prefix" name="url_prefix" value="/financial_assets" />
				</form>	