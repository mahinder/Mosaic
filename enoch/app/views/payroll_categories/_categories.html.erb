<form class="block-content form" id="table_form" method="post" action="">
					<h1>Payroll Category List</h1>
					<div>
						<ul class="tabs js-tabs">
							<li class="current"><a href="#tab-active" title="Active">Active</a></li>
							<li><a href="#tab-inactive" title="Inactive">Inactive</a></li>
						</ul>
					</div>
					<div id="tabs-content" class="tabs-content">
						<div id="tab-active">
							<table id="earning-active-table" class="table earningpayrolls no-margin" cellspacing="0" width="100%">
								<thead>
									<%= render 'table_head' %>
								</thead>
								<tbody>		
									<% @all_active_categories.each do |category| %>
										
											<%= render 'table_row', :category => category %>
										
									<% end %>
								</tbody>
							</table>
						</div>
						<div id="tab-inactive">
							<table id="earning-inactive-table" class="table earningpayrolls no-margin" cellspacing="0" width="100%">
								<thead>
									<%= render 'table_head' %>
								</thead>
								<tbody>		
									<% @all_inactive_categories.each do |category| %>
										
											<%= render 'table_row', :category => category %>
										
									<% end %>
								</tbody>
							</table>
						</div>
					</div>
					<input type="hidden" id="url_prefix" name="url_prefix" value="/payroll_categories" />
				</form>	



